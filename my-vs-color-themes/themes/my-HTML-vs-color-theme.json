{
	"name": "my-HTML-vs-color-theme",
	"type": "dark",
	"tokenColors": [
		{ // Comment 
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		{ // All Tags 
			"scope": [
				"entity.name.tag.html"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF1493"
			}
		},
		{ // Tag Begin End 
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffffff"
			}
		},
		{ // All Attributes 
			"scope": [
				"meta.attribute"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00ffff"
			}
		},
		{ // doctype HTML 
			"scope": [
				"meta.tag.metadata.doctype.html"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FFA500"
			}
		},
		{ // Strings 
			"scope": [
				"string.quoted.double.html"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00FA9A"
			}
		},
		{ // Text 
			"scope": [
				"text.html.derivative",
				"punctuation.separator.key-value.html"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffffff"
			}
		},
	]
}