{
	"name": "my-JS-vs-color-theme",
	"type": "dark",
	"tokenColors": [
		{ // Comment 
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		// "white":            "white",						#ffffff
		// "comment":          "#808080",					#808080
		// "operatorLight":    "palegoldenrod",				#EEE8AA
		// "operatorMedium":   "khaki",						#F0E68C
		// "operatorDark":     "yellow",					#FFFF00
		// "keyword":          "deeppink",					#FF1493
		// "string":           "springgreen",				#00FF7F
		// "tsexpressionb":    "rgba(124, 124, 124, 0.2)",	#7c7c7c33
		// "tsexpressionf":    "rgba(124, 124, 124, 0.2)",	#7c7c7c33
		// "constant":         "violet",					#EE82EE
		// "function":         "cyan",						#00FFFF
		// "functionIO":       "orange",					#FFA500
		// "objectProperty":   "#ff9999",					#ff9999
		// "this":             "khaki",						#F0E68C
		// "class":            "#F1FF7B",					#F1FF7B
		// "domObject":        "mediumspringgreen"			#00FA9A
		{ // Keywords 
			"scope": [
				"keyword.control",
				"storage.modifier.async.js",
				"storage.type.js",
				"keyword.operator.expression.typeof.js",
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.end.js",
				"keyword.operator.new.js",
				"storage.type.function.js",
				"storage.type.class.js",
				"storage.modifier.js",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FF1493"
			}
		},
		{ // Strings 
			"scope": [
				"string.quoted.single.js",
				"string.quoted.double.js",
				"string.template.js",
				"punctuation.definition.string.begin.js",
				"punctuation.definition.string.end.js",
				"string.regexp.js",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00FA9A"
			}
		},
		{ // Alias 
			"scope": [
				"variable.other.readwrite.alias.js",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F1FF7B"
			}
		},
		{ // Punctuation | White 
			"scope": [
				"punctuation.definition.parameters.begin.js",
				"punctuation.definition.parameters.end.js",
				"meta.brace.round.js",
				"punctuation.definition.block.js",
				"variable.other.readwrite.js",
				"variable.other.constant.js",
				"punctuation.separator.key-value.js",
				"punctuation.terminator.statement.js",
				"meta.brace.square.js",
				"punctuation.accessor.js",
				"punctuation.separator.comma.js",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffffff"
			}
		},
		{ // Orange 
			"scope": [
				"keyword.control.trycatch.js",
				"variable.parameter.js",
				"punctuation.definition.character-class.regexp",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FFA500"
			}
		},
		{ // Functions 
			"scope": [
				"storage.type.function.arrow.js",
				"entity.name.function.js",
				"support.function.js",
				"entity.name.type.js",
				"support.function.json.js",
				"variable.language.super.js",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00FFFF"
			}
		},
		{ // Operators Light
			"scope": [
				"keyword.operator.assignment.js",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#EEE8AA"
			}
		},
		{ // Operators Medium 
			"scope": [
				"keyword.operator.comparison.js",
				"keyword.operator.assignment.compound.js",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F0E68C"
			}
		},
		{ // Operators Dark 
			"scope": [
				"keyword.operator.logical.js",
				"keyword.operator.ternary.js",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFF00"
			}
		},
		{ // Objects Keys
			"scope": [
				"meta.object-literal.key.js",
				"variable.other.object.property.js",
				"variable.other.property.js",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ff9999"
			}
		},
		{ // Other Objects 
			"scope": [
				"variable.other.object.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffffff"
			}
		},
		{ // Constants 
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character.escape.backslash.regexp",
				"support.constant.json.js",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#EE82EE"
			}
		},
		{ // Module 
			"scope": [
				"support.type.object.module.js"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FF69B4"
			}
		},
		{ // Env Variables 
			"scope": [
				"variable.other.constant.property.js",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#EE82EE"
			}
		},
		{ // Support Variable 
			"scope": [
				"support.variable.property.js",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff9999"
			}
		},
		{ // Destructuring 
			"scope": [
				"punctuation.definition.binding-pattern.object.js",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#F0E68C"
			}
		},
		{ // Support Class 
			"scope": [
				"entity.name.type.module.js",
				"entity.name.type.class.js",
				"support.class",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F1FF7B"
			}
		},
		{ // This 
			"scope": [
				"variable.language.this.js",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F0E68C"
			}
		},
		{ // test 
			"scope": [
				"test"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ff0000"
			}
		},
	]
}