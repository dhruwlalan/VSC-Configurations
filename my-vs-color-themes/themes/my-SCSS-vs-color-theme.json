{
	"name": "my-SCSS-vs-color-theme",
	"type": "dark",
	"tokenColors": [
		{ // Comment 
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		{ // * 
			"scope": [
				"entity.name.tag.wildcard.scss",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FF1493"
			}
		},
		{ // Tag Selector 
			"scope": [
				"entity.name.tag.css",
				"meta.at-rule.keyframes.scss"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FF1493"
			}
		},
		{ // Class Selector 
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#00BFFF"
			}
		},
		{ // ID Selector 
			"scope": [
				"entity.other.attribute-name.id.css",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#EE82EE"
			}
		},
		{ // Placeholders
			"scope": [
				"entity.other.attribute-name.placeholder.css",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#6adfff"
			}
		},
		{ // Parent Selector Reference 
			"scope": [
				"entity.name.tag.reference.scss"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF1493"
			}
		},
		{ // Parent Selector Suffix 
			"scope": [
				"entity.other.attribute-name.parent-selector-suffix.css"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00BFFF"
			}
		},
		{ // Pseudo Element 
			"scope": [
				"entity.other.attribute-name.pseudo-element.css",
				"entity.other.attribute-name.pseudo-class.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F0E68C"
			}
		},
		{ // Bracket Begin End 
			"scope": [
				"punctuation.section.property-list.begin.bracket.curly.scss",
				"punctuation.section.property-list.end.bracket.curly.scss",
				"punctuation.terminator.rule.scss",
				"punctuation.terminator.rule.css",
				"punctuation.separator.key-value.scss",
				"meta.at-rule.include.scss",
				"meta.property-value.scss",
				"punctuation.definition.parameters.begin.bracket.round.scss",
				"punctuation.definition.parameters.end.bracket.round.scss",
				"punctuation.definition.media-query.begin.bracket.round.scss",
				"punctuation.definition.media-query.end.bracket.round.scss",
				"punctuation.section.function.scss",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffffff"
			}
		},
		{ // Strings 
			"scope": [
				"string.quoted.double.scss",
				"string.quoted.single.scss"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00FA9A"
			}
		},
		{ // Property Name 
			"scope": [
				"support.type.property-name.css",
				"support.type.vendored.property-name.css",
				"support.type.property-name.media.css",
				"meta.property-name.scss",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#4bfdb9"
			}
		},
		{ // Property Value 
			"scope": [
				"meta.property-value.scss",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e8d1ff"
			}
		},
		{ // Property Value Constant 
			"scope": [
				"support.constant.property-value.css",
				"support.constant.font-name.css",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FFF969"
			}
		},
		{ // Property Value Numbers 
			"scope": [
				"constant.numeric.css"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffffff"
			}
		},
		{ // Property Value Units 
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FF1493"
			}
		},
		{ // Operator 
			"scope": [
				"keyword.control.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FFF969"
			}
		},
		{ // Namespace 
			"scope": [
				"variable.language.expanded-namespace.scss"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FF1493"
			}
		},
		{ // @use @include 
			"scope": [
				"keyword.control.at-rule.use.scss",
				"keyword.control.at-rule.include.scss",
				"keyword.control.at-rule.forward.scss",
				"keyword.control.at-rule.mixin.scss",
				"keyword.control.if.scss",
				"keyword.control.else.scss",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FF1493"
			}
		},
		{ // @media @keyframe
			"scope": [
				"keyword.control.at-rule.media.scss",
				"keyword.control.at-rule.keyframes.scss"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#f5a3e7"
			}
		},
		{ // Sass Variable 
			"scope": [
				"variable.scss"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00ffff"
			}
		},
		{ // Sass Argument Name 
			"scope": [
				"argument.name.scss"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#4bfdb9"
			}
		},
		{ // Sass Function 
			"scope": [
				"entity.name.function.scss"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFA07A"
			}
		},
		{ // Operator 
			"scope": [
				"keyword.operator",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FAFAD2"
			}
		},
		{ // Null 
			"scope": [
				"constant.null.scss",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9F9C42"
			}
		},
		{ // Important 
			"scope": [
				"keyword.other.important.scss",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF6347"
			}
		},
		{ // Support Functions 
			"scope": [
				"support.function.misc.scss"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFA500"
			}
		},
		{ // Hex Color 
			"scope": [
				"constant.other.color.rgb-value.hex.css",
				"support.constant.color.w3c-standard-color-name.css",
				"support.constant.color.w3c-extended-color-name.css",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#EE82EE"
			}
		},
	]
}