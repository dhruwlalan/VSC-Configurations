{
    "name": "my-AIO-vs-color-theme",
    "type": "dark",
    "tokenColors": [
        ///GENERAL COLORS                                           
        // white: #FFFFFF
        // black: #000000
        // deepPink: #FF1493
        // hotPink: #FF69B4
        // mediumSpringGreen: #00FA9A
        // violet: #EE82EE
        // magenta: #FF00FF
        // orange: #FFA500
        // lightSalmon: #FFA07A
        // yellow: #FFFF00
        ///CUSTOM COLORS                                            
        // objects property name: #FF9999
        // null: #9F9C42
        // css property name: #37FFB2
        // css property value: #FFF969
        // operator light: #EEE8AA
        // operator medium: #F0E68C
        // operator dark: #FFFF00
        // class yellow: #F1FF7B
        // class yellow light: #F7FFB3
        ///GLOBAL                                                   
        { //#Comment   
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#808080"
            }
        },
        ///ENV                                                      
        { //#Key   
            "scope": [
                "variable.other.makefile"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        { //#Seperator   
            "scope": [
                "punctuation.separator.key-value.makefile"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FF1493"
            }
        },
        { //#Value   
            "scope": [
                "source.makefile"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00FA9A"
            }
        },
        ///JSON                                                     
        { //#Puntuations   
            "scope": [
                "punctuation.definition.dictionary.begin.json",
                "punctuation.definition.dictionary.end.json",
                "punctuation.definition.array.begin.json",
                "punctuation.definition.array.end.json",
                "punctuation.separator.dictionary.key-value.json",
                "punctuation.separator.dictionary.pair.json"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        { //#Property Name   
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FF9999"
            }
        },
        { //#String Value   
            "scope": [
                "string.quoted.double.json"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00FA9A"
            }
        },
        { //#Constant Value   
            "scope": [
                "constant.numeric.json",
                "constant.language.json"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#EE82EE"
            }
        },
        ///HTML                                                     
        { //#Puntuations   
            "scope": [
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.separator.key-value.html",
                "text.html.derivative",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        { //#Strings   
            "scope": [
                "string.quoted.double.html"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00FA9A"
            }
        },
        { //#doctype HTML   
            "scope": [
                "meta.tag.metadata.doctype.html entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FFA500"
            }
        },
        { //#Generic Tags   
            "scope": [
                "entity.name.tag.html"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF1493"
            }
        },
        { //#Generic Attributes   
            "scope": [
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00ffff"
            }
        },
        ///CSS SCSS                                                 
        { //#Puntuations   
            "scope": [
                "punctuation.terminator.rule.css",
                "punctuation.terminator.rule.scss",
                "punctuation.separator.key-value.scss",
                "punctuation.separator.key-value.css",
                "punctuation.separator.delimiter.scss",
                "punctuation.definition.parameters.begin.bracket.round.scss",
                "punctuation.definition.parameters.begin.bracket.round.css",
                "punctuation.definition.parameters.end.bracket.round.scss",
                "punctuation.definition.parameters.end.bracket.round.css",
                "punctuation.definition.media-query.begin.bracket.round.scss",
                "punctuation.definition.media-query.end.bracket.round.scss",
                "punctuation.section.property-list.begin.bracket.curly.scss",
                "punctuation.section.property-list.begin.bracket.curly.css",
                "punctuation.section.property-list.end.bracket.curly.scss",
                "punctuation.section.property-list.end.bracket.curly.css",
                "punctuation.section.keyframes.begin.scss",
                "punctuation.section.keyframes.begin.css",
                "punctuation.section.keyframes.end.scss",
                "punctuation.section.keyframes.end.css",
                "punctuation.section.function.scss",
                "meta.at-rule.mixin.scss",
                "source.css.scss"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        { //#Strings   
            "scope": [
                "string.quoted.double.scss",
                "string.quoted.single.scss"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00FA9A"
            }
        },
        { //#Scss @ rules   
            "scope": [
                "keyword.control.at-rule.use.scss",
                "keyword.control.at-rule.include.scss",
                "keyword.control.at-rule.forward.scss",
                "keyword.control.at-rule.mixin.scss",
                "keyword.control.if.scss",
                "keyword.control.else.scss",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FF1493"
            }
        },
        { //#Css @ rules   
            "scope": [
                "keyword.control.at-rule.media.scss",
                "keyword.control.at-rule.keyframes.scss",
                "keyword.control.at-rule.fontface.scss"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF9999"
            }
        },
        { //#Mixin Name   
            "scope": [
                "meta.at-rule.include.scss entity.name.function.scss"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF9999"
            }
        },
        { //#Built-in Functions   
            "scope": [
                "support.function.misc.scss"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FFA500"
            }
        },
        { //#Variable   
            "scope": [
                "variable.scss"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00FFFF"
            }
        },
        { //#Null   
            "scope": [
                "constant.null.scss",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#9F9C42"
            }
        },
        { //#Interpolation   
            "scope": [
                "punctuation.definition.interpolation.begin.bracket.curly.scss",
                "punctuation.definition.interpolation.end.bracket.curly.scss"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFFF00"
            }
        },
        { //#Property Name   
            "scope": [
                "argument.name.scss",
                "support.type.property-name.css",
                "support.type.vendored.property-name.css",
                "meta.property-list.scss entity.name.tag.css",
                "support.type.property-name.media.css",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#37FFB2"
            }
        },
        { //#Property Value   
            "scope": [
                "meta.property-value.scss",
                "support.constant.property-value.css",
                "support.constant.font-name.css",
                "meta.at-rule.keyframes.scss",
                "entity.name.function.scss"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFF969"
            }
        },
        { //#Property Name in Value   
            "scope": [
                "meta.property-value.scss support.type.property-name.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#37FFB2"
            }
        },
        { //#Operator Light   
            "scope": [
                "keyword.operator.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#EEE8AA"
            }
        },
        { //#Operator Medium   
            "scope": [
                "keyword.operator.comparison.scss"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0E68C"
            }
        },
        { //#Operator Dark   
            "scope": [
                "meta.at-rule.if.scss support.constant.property-value.css",
                "meta.at-rule.else.scss support.constant.property-value.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FFFF00"
            }
        },
        { //#Keyframes Stops   
            "scope": [
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#EE82EE"
            }
        },
        { //#Numbers   
            "scope": [
                "constant.numeric.css"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#ffffff"
            }
        },
        { //#Units   
            "scope": [
                "keyword.other.unit"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FF1493"
            }
        },
        { //#Tag Selector   
            "scope": [
                "entity.name.tag.css",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF1493"
            }
        },
        { //#Class Selector   
            "scope": [
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#00BFFF"
            }
        },
        { //#ID Selector   
            "scope": [
                "entity.other.attribute-name.id.css",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#EE82EE"
            }
        },
        { //#Parent Selector &   
            "scope": [
                "entity.name.tag.reference.scss"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF1493"
            }
        },
        { //#Parent Selector Suffix   
            "scope": [
                "entity.other.attribute-name.parent-selector-suffix.css"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00BFFF"
            }
        },
        { //#Pseudo Element Class   
            "scope": [
                "entity.other.attribute-name.pseudo-element.css",
                "entity.other.attribute-name.pseudo-class.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0E68C"
            }
        },
        { //#Namespace   
            "scope": [
                "variable.language.expanded-namespace.scss"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF1493"
            }
        },
        { //#Colors   
            "scope": [
                "constant.other.color.rgb-value.hex.css",
                "support.constant.color.w3c-standard-color-name.css",
                "support.constant.color.w3c-extended-color-name.css",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#EE82EE"
            }
        },
        { //#Placeholders   
            "scope": [
                "entity.other.attribute-name.placeholder.css",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#6adfff"
            }
        },
        { //#Important   
            "scope": [
                "keyword.other.important.scss",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF6347"
            }
        },
        { //#*   
            "scope": [
                "entity.name.tag.wildcard.scss",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF1493"
            }
        },
        ///JS                                                       
        { //#Punctuations   
            "scope": [
                "punctuation.terminator.statement.js",
                "punctuation.separator.key-value.js",
                "punctuation.separator.comma.js",
                "punctuation.definition.parameters.begin.js",
                "punctuation.definition.parameters.end.js",
                "punctuation.definition.block.js",
                "punctuation.accessor.js",
                "variable.other.readwrite.js",
                "variable.other.constant.js",
                "meta.brace.round.js",
                "meta.brace.square.js",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#ffffff"
            }
        },
        { //#Strings   
            "scope": [
                "punctuation.definition.string.begin.js",
                "punctuation.definition.string.end.js",
                "string.quoted.single.js",
                "string.quoted.double.js",
                "string.template.js",
                "string.regexp.js",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00FA9A"
            }
        },
        { //#Keywords   
            "scope": [
                "storage.modifier.js",
                "keyword.control",
                "storage.modifier.async.js",
                "storage.type.js",
                "keyword.operator.expression.typeof.js",
                "keyword.operator.new.js",
                "storage.type.function.js",
                "storage.type.class.js",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FF1493"
            }
        },
        { //#Classes   
            "scope": [
                "variable.other.readwrite.alias.js",
                "variable.other.constant.js",
                "constant.language.import-export-all.js",
                "meta.function-call.js support.constant"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#F1FF7B"
            }
        },
        { //#Imported Classes   
            "scope": [
                "variable.other.object.js"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#F7FFB3"
            }
        },
        { //#Orange   
            "scope": [
                "variable.parameter.js",
                "keyword.control.trycatch.js",
                "punctuation.definition.character-class.regexp",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFA500"
            }
        },
        { //#Operators Light   
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment.js",
                "keyword.operator.relational.js"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#EEE8AA"
            }
        },
        { //#Operators Medium   
            "scope": [
                "keyword.operator.comparison.js",
                "keyword.operator.assignment.compound.js",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0E68C"
            }
        },
        { //#Operators Dark   
            "scope": [
                "keyword.operator.logical.js",
                "keyword.operator.ternary.js",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FFFF00"
            }
        },
        { //#Constants   
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character.escape.backslash.regexp",
                "support.constant.json.js",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#EE82EE"
            }
        },
        { //#Functions   
            "scope": [
                "storage.type.function.arrow.js",
                "entity.name.function.js",
                "support.function.js",
                "entity.name.type.js",
                "support.function.json.js",
                "variable.language.super.js",
                "meta.function-call.js support.function"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00FFFF"
            }
        },
        { //#Import Export   
            "scope": [
                "keyword.control.import.js",
                "keyword.control.from.js",
                "keyword.control.as.js",
                "keyword.control.export.js",
                "keyword.control.default.js"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FF69B4"
            }
        },
        { //#Objects Keys   
            "scope": [
                "meta.object-literal.key.js",
                "variable.other.object.property.js",
                "variable.other.property.js",
            ],
            "settings": {
                "foreground": "#ff9999"
            }
        },
        { //#Module   
            "scope": [
                "support.type.object.module.js"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FF69B4"
            }
        },
        { //#Env Variables   
            "scope": [
                "variable.other.constant.property.js",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#EE82EE"
            }
        },
        { //#Support Variable   
            "scope": [
                "support.variable.property.js",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ff9999"
            }
        },
        { //#Destructuring   
            "scope": [
                "punctuation.definition.binding-pattern.object.js",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#F0E68C"
            }
        },
        { //#Support Class   
            "scope": [
                "entity.name.type.module.js",
                "entity.name.type.class.js",
                "support.class",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F1FF7B"
            }
        },
        { //#This   
            "scope": [
                "variable.language.this.js",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F0E68C"
            }
        },
        ///VUE                                                      
        { //#Puntuations   
            "scope": [
                "",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#ffffff"
            }
        },
        { //#Strings   
            "scope": [
                "text.html.vue-html string.quoted.double.html",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#F0E68C"
            }
        },
        { //#Template Script Style Tag   
            "scope": [
                "entity.name.tag.template.html",
                "entity.name.tag.script.html",
                "entity.name.tag.style.html",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#00FA9A"
            }
        },
        { //#Generic Html Tags   
            "scope": [
                "entity.name.tag.html",
                "generic.tag.block.html",
                "generic.tag.inline.html"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF1493"
            }
        },
        { //#Builtin Tag   
            "scope": [
                "vue.builtin.tag.block.html",
                "vue.builtin.tag.inline.html",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#56ffbb"
            }
        },
        { //#Builtin Tag Begin End   
            "scope": [
                "vue.builtin.tag.block.begin.html",
                "vue.builtin.tag.block.end.html",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#56ffbb"
            }
        },
        { //#Component Tag   
            "scope": [
                "vue.component.tag.html"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF69B4"
            }
        },
        { //#Interpolation   
            "scope": [
                "punctuation.definition.generic.begin.html",
                "punctuation.definition.generic.end.html",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#00FA9A"
            }
        },
        { //#Generic Html Attributes   
            "scope": [
                "generic.attribute.html"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00ffff"
            }
        },
        { //#Vue Attributes   
            "scope": [
                "vue.attribute.html",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#00FA9A"
            }
        },
        { //#Vue Attribute String   
            "scope": [
                "vue.attribute.string.begin.html",
                "vue.attribute.string.end.html",
                "source.directive.vue punctuation.definition.block.js"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#00FA9A"
            }
        },
        { //#Vue Attributes Seperators   
            "scope": [
                "vue.attribute.seperator.html",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#7affcc"
            }
        },
        { //#Vue Attribute Key Value Seperator   
            "scope": [
                "vue.attribute.seperator.key-value.html",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#7affcc"
            }
        },
    ]
}